{
  "name": "senate-game",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "express": "^4.16.2",
    "mission-game-server": "git+https://github.com/acbabis/mission-game-server.git",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-scripts": "1.0.14",
    "socket.io": "^2.0.3"
  },
  "scripts": {
    "start": "node ./server.js",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "dev-start": "concurrently \"npm run dev-client\" \"npm run dev-server\"",
    "dev-client": "react-scripts start",
    "dev-server": "node ./node_modules/mission-game-server/src/server.js"
  },
  "proxy": {
    "/": {
      "target": "http://localhost:3001"
    },
    "/sockjs-node": {
      "target": "ws://localhost:3001",
      "ws": true
    },
    "/socket.io": {
      "target": "ws://localhost:3001",
      "ws": true
    }
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  }
}
